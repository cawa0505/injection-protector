{
    "name": "protect/sql-query-protection",
    "description": "A Laravel middleware for SQL and XSS protection",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aswin Sasi",
            "email": "aswinfear01@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^5.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "SqlQueryProtection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SqlQueryProtection\\SqlQueryProtectionServiceProvider"
            ],
            "aliases": {
                "SqlQueryProtection": "Facades\\SqlQueryProtectionFacade"
            }
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    }
}
